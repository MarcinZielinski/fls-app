<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.151" fx:controller="com.fls.forum.controller.EditorController">
    <children>
        <TextArea fx:id="textArea" layoutX="36.0" layoutY="23.0" prefHeight="267.0" prefWidth="526.0"/>
        <Button fx:id="okButton" onAction="#handleOkAction" layoutX="130.0" layoutY="308.0" mnemonicParsing="false"
                prefHeight="52.0" prefWidth="102.0" text="OK"/>
        <Button fx:id="cancelButton" onAction="#handleCancelAction" layoutX="331.0" layoutY="308.0"
                mnemonicParsing="false" prefHeight="52.0" prefWidth="102.0" text="Cancel"/>
    </children>
</AnchorPane>
