<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="roomPane" prefHeight="600.0" AnchorPane.topAnchor="0.0"
            AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="com.fls.chat.controller.RoomController">
    <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <top>
            <Pane prefHeight="112.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                <BorderPane.margin>
                    <Insets/>
                </BorderPane.margin>
                <Text fx:id="roomName" layoutX="20.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0">
                    <font>
                        <Font name="DejaVu Sans ExtraLight" size="19.0"/>
                    </font>
                </Text>
            </Pane>
        </top>
        <center>
            <ListView fx:id="messagesView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
        </center>
    </BorderPane>
</AnchorPane>
